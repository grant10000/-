    print(int(s[0]))
if int(s[1]) > int(s[0]) and int(s[1])>int(s[2]):
    prnit(int(s[1]))
if int(s[2]) > int(s[1]) and int(s[2]) > int(s[0]):
    print(int(s[2]))

