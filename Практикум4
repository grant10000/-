#5
fish1=int(input('Введите улов первого рыбака '))
fish2=int(input('Введите улов второго рыбака '))
if fish1>fish2:
    print(fish2)
else:
    print(fish1)

#6
play1=input('Введите счет ')
p=play1.split(':')
if p[0]>p[1]:
    print(1)
if p[0]<p[1]:
    print(2)
if p[0]==p[1]:
    print(0)

