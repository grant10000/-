#9
i1=input('Собака короткошерстной породы? ')
if i1=='да':
    i22=input('Рост собаки менее 50 см? ')
    if i22=='да':
       i2=input('У собаки короткий хвост? ')
       if i2=='да':
           print('английский бульдог')
       if i2=='нет':
           i3=input('У собаки длинные уши? ')
           if i3=='да':
               print('гончая')
           if i3 == 'нет':
               i4=input('У собаки короткое тело? ')
               if i4 == 'да':
                   print('мопс')
               if i4 == 'нет':
                   print('чихуахуа')
    if i22=='нет':
        i5=input('Собака весит более 50 кг? ')
        if i5=='да':
            print('датский дог')
        if i5 == 'нет':
            print('фоксхаунд')


if i1=='нет':
    i6=input('Рост собаки менее 50 см ? ')
    if i6 == 'да':
        i7=input('У собаки доброжелательный характер? ')
        if i7=='да':
            print('кокер-спаниэль')
        if i7 == 'нет':
            print('ирландский сеттер')

    if i6 == 'нет':
        i8=input('У собаки рост менее 70 см ? ')
        if i8 == 'да':
            i9 = input('У собаки длинные уши ? ')
            if i9 == 'да':
                print('большой вандейский гифон')
            if i9 == 'нет':
                print('колли')
        if i8 == 'нет':
            i10 = input('Окраска рыжий с белыми отметинами? ')
            if i10 == 'да':
                print('сенбернар')
            if i10== 'нет':
                i11= input('Белоснежный окрас? ')
                if i11 == 'да':
                    print('ирландский волкодав')
                if i9 == 'нет':
                    print('ньюфаундленд')



